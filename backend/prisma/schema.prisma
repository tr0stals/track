generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Sugar {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
}

model Pressure {
  id        Int      @id @default(autoincrement())
  value     Float
  createdAt DateTime @default(now())
}

model Pulse {
  id        Int      @id @default(autoincrement())
  value     Int
  createdAt DateTime @default(now())
}

